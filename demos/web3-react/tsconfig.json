{
  "compilerOptions": {
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": false,
    "target": "ES2021",
    "module": "ES2022",
    "moduleResolution": "node",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmit": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "paths": {
      "src": ["./src"],
      "src/*": ["./src/*"]
    }
  },
  "include": ["src"]
}
